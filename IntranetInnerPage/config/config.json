{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "add-list-item-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/addListItem/AddListItemWebPart.js",
          "manifest": "./src/webparts/addListItem/AddListItemWebPart.manifest.json"
        }
      ]
    },
    "listview-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/listview/ListviewWebPart.js",
          "manifest": "./src/webparts/listview/ListviewWebPart.manifest.json"
        }
      ]
    },
    "edit-list-item-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editListItem/EditListItemWebPart.js",
          "manifest": "./src/webparts/editListItem/EditListItemWebPart.manifest.json"
        }
      ]
    },
    "view-list-item-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/viewListItem/ViewListItemWebPart.js",
          "manifest": "./src/webparts/viewListItem/ViewListItemWebPart.manifest.json"
        }
      ]
    },
    "image-gallery-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageGallery/ImageGalleryWebPart.js",
          "manifest": "./src/webparts/imageGallery/ImageGalleryWebPart.manifest.json"
        }
      ]
    },
    "image-gallery-detail-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/imageGalleryDetail/ImageGalleryDetailWebPart.js",
          "manifest": "./src/webparts/imageGalleryDetail/ImageGalleryDetailWebPart.manifest.json"
        }
      ]
    },
    "video-gallery-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/videoGallery/VideoGalleryWebPart.js",
          "manifest": "./src/webparts/videoGallery/VideoGalleryWebPart.manifest.json"
        }
      ]
    },
    "organization-policy-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/organizationPolicy/OrganizationPolicyWebPart.js",
          "manifest": "./src/webparts/organizationPolicy/OrganizationPolicyWebPart.manifest.json"
        }
      ]
    },
    "corporate-discount-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/corporateDiscount/CorporateDiscountWebPart.js",
          "manifest": "./src/webparts/corporateDiscount/CorporateDiscountWebPart.manifest.json"
        }
      ]
    },
    "video-gallery-collections-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/videoGalleryCollections/VideoGalleryCollectionsWebPart.js",
          "manifest": "./src/webparts/videoGalleryCollections/VideoGalleryCollectionsWebPart.manifest.json"
        }
      ]
    },
    "edit-list-items-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/editListItems/EditListItemsWebPart.js",
          "manifest": "./src/webparts/editListItems/EditListItemsWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js",
      "globalName": "jquery"
    },
    "jplist": {
      "path": "./jplist/1.2.0/jplist.js",
      "globalName": "jplist"
    },
    "jplist-core": {
      "path": "./src/ExternalRef/js/jplist-core.js",
      "globalName": "jplist-core",
      "globalDependencies": [
        "jquery"
      ]
    },
    "bootstrap": {
      	      "path": "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js",
      	      "globalName": "bootstrap",
    	      "globalDependencies": [
    	        "jquery"
    	      ]
         		    
        },
    "jplist-pagination": {
      "path": "./src/ExternalRef/js/jplist-pagination.js",
      "globalName": "jplist-pagination",
      "globalDependencies": [
        "jquery"
      ]
    },
    "bootstrap-select.min": {
      "path": "./src/ExternalRef/js/bootstrap-select.min.js",
      "globalName": "bootstrap-select.min",
      "globalDependencies": [
        "jquery"
      ]
    }
  },
  "localizedResources": {
    "AddListItemWebPartStrings": "lib/webparts/addListItem/loc/{locale}.js",
    "ListviewWebPartStrings": "lib/webparts/listview/loc/{locale}.js",
    "EditListItemWebPartStrings": "lib/webparts/editListItem/loc/{locale}.js",
    "ViewListItemWebPartStrings": "lib/webparts/viewListItem/loc/{locale}.js",
    "ImageGalleryWebPartStrings": "lib/webparts/imageGallery/loc/{locale}.js",
    "ImageGalleryDetailWebPartStrings": "lib/webparts/imageGalleryDetail/loc/{locale}.js",
    "VideoGalleryWebPartStrings": "lib/webparts/videoGallery/loc/{locale}.js",
    "OrganizationPolicyWebPartStrings": "lib/webparts/organizationPolicy/loc/{locale}.js",
    "CorporateDiscountWebPartStrings": "lib/webparts/corporateDiscount/loc/{locale}.js",
    "VideoGalleryCollectionsWebPartStrings": "lib/webparts/videoGalleryCollections/loc/{locale}.js",
    "EditListItemsWebPartStrings": "lib/webparts/editListItems/loc/{locale}.js"
  }
}
